<div class="container">
    <div class="row">
        <h3 class="m-2">
            Lista de los mejores puntajes
        </h3>
    </div>
    <table class="table m-3">
        <thead class="thead-dark">
            <tr>
                <th scope="col">Nombre</th>
                <th scope="col">Email</th>
                <th scope="col">Puntuation</th>
            </tr>
        </thead>
        <tbody>
            {{#each contactos}}
            <tr>
                <th scope="row"> {{nombre}}</th>
                <td>{{mail}}</td>
                <td>{{highscore}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    <div class="row">
        <div class="col">
            <a href="/challenges/" class="btn btn-primary m-2 w-30">Volver atr√°s</a>
        </div>
    </div>
</div>

